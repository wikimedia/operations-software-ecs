{
  "_meta": {"version": "__VERSION__"},
  "date_detection": false,
  "dynamic_templates": [
    {
      "strings_as_keyword": {
        "mapping": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "match_mapping_type": "string"
      }
    },
    {
      "metrics_puppet_runtime_seconds_half_float": {
        "path_match": "metrics.puppet.runtime.*.seconds",
        "mapping": {
          "type": "half_float"
        }
      }
    }
  ],
  "properties": {}
}
